{
  "name": "gcs-source-transactions",
  "config": {
    "connector.class": "io.confluent.connect.gcs.GcsSourceConnector",
    "tasks.max": "1",

    "mode": "GENERIC",

    "gcs.bucket.name": "confluent-topics",
    "gcs.credentials.path": "/tmp/gcp-sa-key.json",

    "topics.dir": "exported_data",
    "topic.regex.list": "transactions-gcs-to-topic:.*",

    "format.class": "io.confluent.connect.gcs.format.json.JsonFormat",

    "gcs.poll.interval.ms": "60000",

    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "key.converter.schemas.enable": "false",

    "confluent.topic.bootstrap.servers": "broker:29092",
    "confluent.topic.replication.factor": "1",
    "confluent.license": ""
  }
}